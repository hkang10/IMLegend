<div class="admin-control">
  <h3>Admin Control</h3>

  <%= link_to 'Students', students_path %>


<select name="teacher[teacher_id]">
    <% Teacher.all.each do |teacher| %>
        <option value="<%= teacher.id %>"><%= teacher.first_name %> <%= teacher.last_name %></option>
    <% end %>
</select>
</div>
