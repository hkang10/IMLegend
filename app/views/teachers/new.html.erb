<div class="teacher-form">
  <h3>Start your legend...........here</h3>
  <div class="error-container">
    <%= render 'partials/errors', layout: false %>
  </div>
  <%= form_for(@teacher, remote: true) do |f| %>
  <p>
   <%= f.label :first_name %>
   <%= f.text_field :first_name %>
  </p>
  <p>
   <%= f.label :last_name %>
   <%= f.text_field :last_name %>
  </p>
  <p>
   <%= f.label :email %>
   <%= f.email_field :email %>
  </p>
  <p>
   <%= f.label :password %>
   <%= f.password_field :password %>
  </p>
  <p>
    Choose a Team Name <%= f.text_field :team_name, value: ''%>
  </p>
  <p>
   Access Code <%= f.text_field :access_code, value: '' %>
 </p>
 <%= f.submit %>
 <% end %>
</div>
