<div class="student-admin-list">
<h2>Student Roster</h2>

<% @students.each do |student| %>
  <ul>
    <li>
      <%= student.first_name %> <%= student.last_name %>
      <%= link_to "Remove Student", student_path(student) , method: :delete, data: { confirm: "Are you sure?" } %> <br>
    </li>
  </ul>
<% end %>

</div>
